{
  "name": "ecma-utils",
  "version": "0.1.3",
  "description": "A collection of ECMAScript utilities for use across my projects.",
  "author": "Michael Rosata",
  "contributors": [
    {
      "name": "Michael Rosata",
      "email": "mrosata1984@gmail.com"
    }
  ],
  "keywords": [
    "utils",
    "type",
    "validation",
    "helper",
    "functions"
  ],
  "main": "dist/ecma-utils.js",
  "module": "dist/ecma-utils.mjs",
  "jsnext:main": "dist/ecma-utils.mjs",
  "scripts": {
    "prebuild": "npm run clean && eslint lib test",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "watch": "rollup -c -w",
    "pretest": "npm run build",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "prepublish": "npm test"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "3.5.0",
    "eslint": "3.6.1",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-eslint": "3.0.1",
    "gulp-mocha": "3.0.1",
    "gulp-rimraf": "0.2.0",
    "istanbul": "^0.4.5",
    "mocha": "3.1.0",
    "ramda": "^0.22.1",
    "rimraf": "2.5.4",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "run-sequence": "1.2.2"
  },
  "engine": "node >= 0.10.x",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrosata/ecma-utils.git"
  },
  "files": [
    "lib",
    "dist"
  ],
  "private": false,
  "license": "MIT",
  "dependencies": {
    "babelrc-rollup": "^3.0.0",
    "gulp": "^3.9.1",
    "rollup-plugin-istanbul": "^1.1.0"
  }
}
